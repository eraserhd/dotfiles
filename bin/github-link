#!/bin/sh
#
# Get a browse link for a file in a GitHub repository
#

fileName=''
startLine=''
endLine=''

parseOptions() {
    arg1="$1"
    arg2="$2"
    arg1="${arg1%:}"
    if [ $(expr "$arg1" : '.*:[[:digit:]][[:digit:]]*$') -ne 0 ]; then
    fi
    case "$arg1" in
    *:*-*)    ;;
    *:[0-9]*) ;;
    *)        fileName="$1";;
    esac
}

parseOptions "$@"
